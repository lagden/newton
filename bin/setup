#!/bin/bash

THIS=$(basename $0)
DIR="$( cd -P "$( dirname "$0" )" && pwd )"
cd $DIR
cd ..
CURR_FOLDER=`pwd`

# Go
cd $CURR_FOLDER

if [ -d build ]
then
  cp -r build/. ./
fi

folders="build dev node_modules"
for folder in $folders
do
    if [ -d $folder ]
    then
      rm -rf $folder
    fi
done

exit
