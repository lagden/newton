{
  "bitwise": true,
  "browser": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "esnext": true,
  "expr": true,
  "globals": {
    "console": false,
    "define": false,
    "document": false,
    "expect": false,
    "module": false,
    "require": false,
    "window": false
  },
  "immed": true,
  "indent": 2,
  "latedef": true,
  "maxcomplexity": 1,
  "maxlen": 80,
  "newcap": true,
  "noarg": true,
  "node": true,
  "noempty": true,
  "nonstandard": true,
  "quotmark": "single",
  "regexp": true,
  "smarttabs": true,
  "strict": true,
  "trailing": true,
  "undef": true,
  "unused": true
}
